<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="UCExportDataOption" TargetType="{x:Type UserControl}">
        <Setter Property="MinWidth" Value="350"></Setter>
        <Setter Property="MinHeight" Value="250"></Setter>
    </Style>
    
    <Style x:Key="BorderMain" TargetType="{x:Type Border}">
        <Setter Property="Margin" Value="15,20,15,5"></Setter>
    </Style>
    
</ResourceDictionary>
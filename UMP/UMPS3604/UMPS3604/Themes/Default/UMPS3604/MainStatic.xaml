<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:conv="clr-namespace:UMPS3604.Converters">
    
    <conv:StrImageConverter x:Key="StrImageConverter" ></conv:StrImageConverter>

    <DataTemplate x:Key="CellUploadStatusTemplate">
        <Image Stretch="None" Source="{Binding StrImage, Converter={StaticResource StrImageConverter}}"></Image>
    </DataTemplate>
</ResourceDictionary>
<UserControl x:Class="UMPS1106.UCSetType103"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <RadioButton x:Name="RadioButtonValue0" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="0"/>
            <RadioButton x:Name="RadioButtonValue1" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="1"/>
            <RadioButton x:Name="RadioButtonValue2" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="2"/>
            <RadioButton x:Name="RadioButtonValue3" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="3"/>
            <RadioButton x:Name="RadioButtonValue4" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="4"/>
            <RadioButton x:Name="RadioButtonValue5" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="5"/>
            <RadioButton x:Name="RadioButtonValue6" Style="{DynamicResource RadioButtonValueWeekStyle}" DataContext="6"/>
        </StackPanel>
    </Grid>
</UserControl>

<Window x:Class="UMP.MAMT.BasicModule.WaitForApplicationDoing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WaitForApplicationDoing" Height="34" Width="400" WindowState="Normal" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" BorderThickness="1" BorderBrush="LightGray" ShowInTaskbar="False" WindowStyle="None">
    <Grid>
        <Border Style="{DynamicResource BorderForApplicationDoing}">
            <Grid>
                <Image Name="ImageStatus" Style="{DynamicResource ImageInWaitForApplication}" />
                <Label Name="LabelStatus" Style="{DynamicResource LabelInWaitForApplication}" Content="112233"/>
            </Grid>
        </Border>
    </Grid>
</Window>

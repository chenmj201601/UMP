<umpc:UMPUserControl x:Class="UMPS1110.UCResourceObjectLister"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:umpc="http://www.voicecyber.com/ump/controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/UMPS1110;component/Themes/Default/UMPS1110/UCResourceObjectLister.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/UMPS1110;component/Themes/Default/UMPS1110/UCResourceObjectListerStatic.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    
    <Grid>
        <ListView Name="LvResourceObjects"
                  SelectionMode="Extended"
                  Style="{DynamicResource ListViewResourceObject}"
                  ItemContainerStyle="{DynamicResource ListViewItemResourceObject}"></ListView>
    </Grid>
</umpc:UMPUserControl>

<umpc:UMPUserControl x:Class="UMPS1110.Editors.UCPropertyEditor420"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:umpc="http://www.voicecyber.com/ump/controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ComboBox Name="ComboNetworkCards" 
                     Style="{DynamicResource EditorComboBoxStyle}"
                     ItemContainerStyle="{DynamicResource EditorComboBoxItemStyle}">
            <ComboBox.ItemTemplate>
                <DataTemplate DataType="{x:Type ComboBoxItem}">
                    <TextBlock Text="{Binding Display}"></TextBlock>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
    </Grid>
</umpc:UMPUserControl>

<umpc:UMPUserControl x:Class="UMPS1110.UCChildObjectLister"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:umpc="http://www.voicecyber.com/ump/controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Default/UMPS1110/UCChildObjectLister.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Themes/Default/UMPS1110/UCChildObjectListerStatic.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ListView Name="LvChildObjects"
                  SelectionMode="Extended"
                  Style="{DynamicResource ListViewChildObject}"
                  ItemContainerStyle="{DynamicResource ListViewItemChildObject}"></ListView>
    </Grid>
</umpc:UMPUserControl>

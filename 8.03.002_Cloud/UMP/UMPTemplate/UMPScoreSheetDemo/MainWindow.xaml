        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ssc="http://www.voicecyber.com/UMP/ScoreSheets/Controls"
        xmlns:vcc="http://www.voicecyber.com/Wpf/Controls"
        Title="UMPScoreSheetDemo" Height="600" Width="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <Menu DockPanel.Dock="Top">
                <MenuItem Name="BtnTest" Header="Test"></MenuItem>
            </Menu>
            <vcc:DoubleUpDown DockPanel.Dock="Top" ValueChanged="UpDownBase_OnValueChanged"></vcc:DoubleUpDown>

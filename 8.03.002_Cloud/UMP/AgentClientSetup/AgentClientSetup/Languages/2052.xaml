<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <!--简体-->
    <s:String x:Key="ServerAddress">服务器地址:</s:String>
    <s:String x:Key="ServerPort">端口:</s:String>
    <s:String x:Key="Install">安裝</s:String>
    <s:String x:Key="Close">关闭</s:String>
    <s:String x:Key="Cancel">取消</s:String>
    <s:String x:Key="UMPHead">安装 UMP 证书</s:String>

    <!--Message-->
    <s:String x:Key="InstallSucessed">安裝成功</s:String>
    <s:String x:Key="Warning">警告</s:String>
    <s:String x:Key="Infomation">提示</s:String>
    <s:String x:Key="Error">错误</s:String>
    <s:String x:Key="ServerInfoIsNull">请输入正确的服务器地址和端口</s:String>


    <!--Error-->
    <s:String x:Key="LoseXmlFile">UMPClientInit.xml文件不存在</s:String>
    <s:String x:Key="AddressIsError">请输入正确的服务器地址</s:String>
    <s:String x:Key="PortIsError">请输入正确的端口</s:String>
    <s:String x:Key="LinkFailed">连接服务器失败，请检查服务器地址、端口输入是否正确，网络是否畅通</s:String>

</ResourceDictionary>
<umpc:UMPUserControl x:Class="UMPS1106.MainUserControl.UCSetType108"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                           xmlns:umpc="http://www.voicecyber.com/ump/controls"
             xmlns:vcc="http://www.voicecyber.com/Wpf/Controls"
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.85*"></ColumnDefinition>
            <ColumnDefinition Width="0.15*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBox Name="TextBoxPath" Style="{DynamicResource NormalFontStyle}" Margin="10,0,0,0"  Height="26" Grid.Column="0"/>
        <Button  x:Name="SelectPath" Grid.Column="1" Style="{DynamicResource ButtonEditStyle}" > </Button>
    </Grid>
</umpc:UMPUserControl>

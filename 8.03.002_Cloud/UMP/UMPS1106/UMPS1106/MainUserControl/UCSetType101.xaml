             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:umpc="http://www.voicecyber.com/ump/controls"
             xmlns:vcc="http://www.voicecyber.com/Wpf/Controls"
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--<ResourceDictionary Source="../Themes/Default/UMPS1106/StyleDictionary1106.xaml"></ResourceDictionary>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <StackPanel Orientation="Horizontal">
            <RadioButton x:Name="RadioButtonValue1" Style="{DynamicResource RadioButtonValue1Style}" DataContext="1"/>
            <RadioButton x:Name="RadioButtonValue0" Style="{DynamicResource RadioButtonValue0Style}" DataContext="0"/>
        </StackPanel>
    </Grid>

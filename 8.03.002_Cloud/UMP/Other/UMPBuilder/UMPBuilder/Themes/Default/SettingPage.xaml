    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Control.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
    
    <Style x:Key="SettingWindowStyle" TargetType="{x:Type Window}">
        

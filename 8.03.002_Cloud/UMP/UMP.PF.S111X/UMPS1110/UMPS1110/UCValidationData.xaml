<UserControl x:Class="UMPS1110.UCValidationData"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="300">
    <Grid x:Name="GridValidationDataPanel" Style="{DynamicResource YUCGridValidationDataPanelStyle}">
        <Image Name="ImageOperationLeft" Style="{DynamicResource YImageOperationGroupLeftStyle}" />
        <TextBlock Name="TextBlockOperationName" Style="{DynamicResource YTextBlockOperationNameNoRightStyle}"/>
    </Grid>
</UserControl>
